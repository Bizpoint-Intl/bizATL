<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DetailGridInfo Name="detailGridInfo" MainTable="por1"	ExtractGridInfo="ExtractGridInfo_PRQE.xml">
		<VirtualColumns>
			<bas:VirtualColumn ColumnName="mark"	DataType="System.Boolean" DefaultValue="False"/>
		</VirtualColumns>
		<DetailGridColumnInfo MappingName="mark"			HeaderText="{languageBinding.mark}"				Width="35"	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="matnum"		HeaderText="ProductNo"			Width="120"  F2="True"  F2GridInfo="F2GridInfo_MATM2_2.xml"/>
    <DetailGridColumnInfo MappingName="apmatnum"		HeaderText="APProductNo"			Width="120"  F2="True"  F2GridInfo="F2GridInfo_MATM2_1.xml"/>
		<DetailGridColumnInfo MappingName="detail"			HeaderText="{languageBinding.detail}"				Width="200" F2="True" F2GridInfo="F2GridInfo_MATM2_3.xml"/>

    <DetailGridColumnInfo MappingName="pqty"			HeaderText="P. QTY"				Width="60"/>
    <DetailGridColumnInfo MappingName="uomcode"			HeaderText="P UOM"				Width="60" F2="True"   F2GridInfo="F2GridInfo_UOM.xml"/>
    <DetailGridColumnInfo MappingName="uomqty"		HeaderText="Convertion Factor"			Width="80"  ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="approveqty"			HeaderText="Appv QTY"				Width="60" ReadOnly="true"/>
    <DetailGridColumnInfo MappingName="qty"			HeaderText="Base QTY"				Width="60" ReadOnly="true"/>
    <DetailGridColumnInfo MappingName="uom"			HeaderText="Base UOM"				Width="60"	ReadOnly="true"/>

    <DetailGridColumnInfo MappingName="uprice"			HeaderText=""			Width="0"/>
		<DetailGridColumnInfo MappingName="price"			HeaderText="{languageBinding.unitprice}"			Width="80"/>
		<DetailGridColumnInfo MappingName="grosamt"		HeaderText="{languageBinding.grosamt}"			Width="80"/>
		<DetailGridColumnInfo MappingName="discamt"		HeaderText="{languageBinding.discamt}"			Width="80"/>
		<DetailGridColumnInfo MappingName="oriamt"		HeaderText="{languageBinding.detail1_oriamt}"	Width="80" 	ReadOnly="true"/>
    <DetailGridColumnInfo MappingName="memo"		HeaderText="Memo"			Width="600"/>
		<DetailGridColumnInfo MappingName="remark"		HeaderText="{languageBinding.remarks}"			Width="300"/>
    
    <DetailGridColumnInfo MappingName="sn"		HeaderText="S/N"			Width="35" ColumnType="CheckBox"/>
    <DetailGridColumnInfo MappingName="whnum"			HeaderText="WHNUM"		Width="80" F2="True" F2GridInfo="F2GridInfo_WHM.xml"/>
    <DetailGridColumnInfo MappingName="trqnum"			HeaderText="MRQ No"		Width="120" F2="True" F2GridInfo="F2GridInfo_TRQH.xml"/>
    <!--<DetailGridColumnInfo MappingName="modelcode"		HeaderText="Convertion Factor"			Width="120"/>-->
    <DetailGridColumnInfo MappingName="pbrdcode"		HeaderText=""			Width="80"  F2="True"  F2GridInfo="F2GridInfo_PBRD.xml"/>
    <DetailGridColumnInfo MappingName="ploftcode"		HeaderText=""			Width="80"  F2="True"  F2GridInfo="F2GridInfo_PLFT.xml"/>
    <DetailGridColumnInfo MappingName="pflexcode"		HeaderText=""			Width="80"  F2="True"  F2GridInfo="F2GridInfo_PFLX.xml"/>
    <DetailGridColumnInfo MappingName="pshfcode"		HeaderText=""			Width="80"  F2="True"  F2GridInfo="F2GridInfo_PSHF.xml"/>
    <DetailGridColumnInfo MappingName="pcolcode"		HeaderText=""			Width="80"  F2="True"  F2GridInfo="F2GridInfo_PCOL.xml"/>
    <DetailGridColumnInfo MappingName="pszcode"		HeaderText=""			Width="80"  F2="True"  F2GridInfo="F2GridInfo_PSZ.xml"/>

    <DetailGridColumnInfo MappingName="baseunitprice"		HeaderText=""			Width="0"/>
    <DetailGridColumnInfo MappingName="boxno"		HeaderText=""			Width="0"/>
		<DetailGridColumnInfo MappingName="refnum"		HeaderText="{languageBinding.refnum}"			Width="0"	ReadOnly="true" Key="True"/>
    <DetailGridColumnInfo MappingName="line"        HeaderText="{languageBinding.matnum}"           Width="0"      Sort="True"/>
	</DetailGridInfo>
</BizXml>
