<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:rel="BizRAD.BizBase, BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DetailGridInfo Name="detailGridInfo" MainTable="mta2" Title="Time Sheet Summary">

		<VirtualColumns>
			<rel:VirtualColumn	ColumnName="mark" DataType="System.Boolean" DefaultValue="False"/>
		</VirtualColumns>
		<DetailGridColumnInfo MappingName="mark"  		  HeaderText="{languageBinding.mark}"		  Width="40" 	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="empnum"			  HeaderText="{languageBinding.empnum}"	 ReadOnly="True" 	Width="70" />
		<DetailGridColumnInfo MappingName="empname"	HeaderText="Name"	 Width="200"     ReadOnly="True"    Format="#,##0.00####"/>

		<DetailGridColumnInfo MappingName="totalpayabledays"	HeaderText="Paid Days"	 Width="70"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="TotalwkHrs"	HeaderText="Work Hrs"	 Width="70"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalrest"	HeaderText="Rest"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totaldre"	HeaderText="DRE"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalot"	HeaderText="OT 2.0"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="ot15hrs"	HeaderText="OT 1.5 Hrs"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="TotalLatecount"	HeaderText="Late Count"	 Width="70"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="TotalLateHrs"	HeaderText="Late Hrs"	 Width="70"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalRest"	HeaderText="Rest"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalAleave"	HeaderText="AL"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalmc"	HeaderText="MC"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalawol"	HeaderText="AWOL"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalupl"	HeaderText="UPL"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="refnum"		HeaderText=""			Width="0"	ReadOnly="true" Key="True"/>
		<DetailGridColumnInfo MappingName="monthno"		HeaderText=""			Width="0"	ReadOnly="true" />
		<DetailGridColumnInfo MappingName="yearno"		HeaderText=""			Width="0"	ReadOnly="true" />
	</DetailGridInfo>
</BizXml>

