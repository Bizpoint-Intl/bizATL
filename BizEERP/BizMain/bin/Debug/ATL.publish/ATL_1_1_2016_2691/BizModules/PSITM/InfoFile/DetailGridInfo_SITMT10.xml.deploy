		<DetailGridColumnInfo MappingName="issueddate"	HeaderText="Issued Date" 	Width="100"     ReadOnly="True"    />
		<DetailGridColumnInfo MappingName="pddesc"	HeaderText="" 	Width="0"     ReadOnly="True"    />
