<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
  <DetailGridInfo Name="detailGridInfo" MainTable="wor5" Title="Location">
    <VirtualColumns>
      <bas:VirtualColumn	ColumnName="mark" DataType="System.Boolean" DefaultValue="False"/>
      <bas:VirtualColumn ColumnName="button1"	DataType="System.String" DefaultValue="Upload" />
      <bas:VirtualColumn ColumnName="button2"	DataType="System.String" DefaultValue="View" />
    </VirtualColumns>
    <DetailGridColumnInfo MappingName="mark"  		  HeaderText="{languageBinding.mark}"		  Width="40" 	ColumnType="CheckBox"/>
    <DetailGridColumnInfo MappingName="remark"	HeaderText="Description"  	Width="450" />
    <DetailGridColumnInfo MappingName="photourl"	HeaderText=""  	Width="" />

    <DetailGridColumnInfo MappingName="button1"		HeaderText="Upload"			Width="80"	ColumnType="TextBox" />
    <DetailGridColumnInfo MappingName="button2"		HeaderText="Preview"			Width="80"	ColumnType="TextBox" />
    <DetailGridColumnInfo MappingName="refnum"		HeaderText=""			Width="0"	ReadOnly="true" Key="True" />
  </DetailGridInfo>
</BizXml>

