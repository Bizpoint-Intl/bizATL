<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DetailGridInfo Name="detailGridInfo" MainTable="regd">
		<VirtualColumns>
			<bas:VirtualColumn ColumnName="mark" DataType="System.Boolean" DefaultValue="False"/>
		</VirtualColumns>
		<DetailGridColumnInfo MappingName="mark"			HeaderText="{languageBinding.mark}"				Width="35"	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="state"			HeaderText="{languageBinding.state}"			Width="200"/>
		<DetailGridColumnInfo MappingName="regnum"			HeaderText="{languageBinding.regnum}"			Width="0"	ReadOnly="True" Key="True"/>
	</DetailGridInfo>
</BizXml>
