<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDocument, BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
  <DocumentInfo Name="documentInfo" MainTable="lvm" Key="lvmnum"   CompanyTitle="{languageBinding.coypg}" ModuleTitle="{languageBinding.LVM}">

	  <RelationTables>
		  <DocRelationTableInfo TableName="lvm" DataField="lvmnum" ConditionField="isCombinableWith" DataTableName="lvm2">
			  <Fields>
				  <Field ColumnName="lvmnum"/>
			  </Fields>
		  </DocRelationTableInfo>

		  <DocRelationTableInfo TableName="lvm" DataField="lvmnum" ConditionField="BroughtForwardTo" DataTableName="lvm3">
			  <Fields>
				  <Field ColumnName="lvmnum"/>
			  </Fields>
		  </DocRelationTableInfo>
	  </RelationTables>
	  
    <DocumentPages>
		<DocumentPageInfo FileName="header.xml" FormName="header" Title="Header">
			<F2Controls>

				<F2Control ControlName="lvm_isCombinableWith" F2GridInfo="F2GridInfo_LVM.xml">
					<RelationF2Tables>
						<RelationF2Table TableName="lvm2"/>
					</RelationF2Tables>
				</F2Control>

				<F2Control ControlName="lvm_BroughtForwardTo" F2GridInfo="F2GridInfo_LVM.xml">
					<RelationF2Tables>
						<RelationF2Table TableName="lvm3"/>
					</RelationF2Tables>
				</F2Control>
				
			</F2Controls>
			
		</DocumentPageInfo>
    </DocumentPages>
  </DocumentInfo>
</BizXml>