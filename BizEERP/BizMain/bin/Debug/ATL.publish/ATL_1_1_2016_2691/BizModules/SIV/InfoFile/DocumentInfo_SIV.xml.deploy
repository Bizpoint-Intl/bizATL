<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDocument,	BizBase"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DocumentInfo Name="documentInfo" MainTable="sivh" Key="refnum" CompanyTitle="{languageBinding.coyDEMO}" ModuleTitle="Sales Invoice">
		<RelationTables>
			<DocRelationTableInfo TableName="siv10" DataField="refnum" ConditionField="refnum" Save="True"/>
      <DocRelationTableInfo TableName="viewprojid"	DataField="projectid" ConditionField="projectid"/>
      <!--<DocRelationTableInfo TableName="prjm"	DataField="projectid" ConditionField="projectid"/>-->

      <DocRelationTableInfo TableName="ard"	DataField="refnum" ConditionField="refnum"/>
      <DocRelationTableInfo TableName="gld"	DataField="refnum" ConditionField="refnum"/>
      <DocRelationTableInfo	TableName="arm" DataField="arnum" ConditionField="arnum">
        <Fields>
          <Field ColumnName="arname"/>
          <!--<Field ColumnName="address"/>-->
          <!--<Field ColumnName="phone"/>
          <Field ColumnName="hp"/>
          <Field ColumnName="fax"/>
          <Field ColumnName="email"/>
          <Field ColumnName="addr1"/>
          <Field ColumnName="addr2"/>
          <Field ColumnName="addr3"/>
          <Field ColumnName="addr4"/>
          <Field ColumnName="saddr1"/>
          <Field ColumnName="saddr2"/>
          <Field ColumnName="saddr3"/>
          <Field ColumnName="saddr4"/>-->
          <Field ColumnName="ptnum"/>
          <Field ColumnName="gstgrpnum"/>
          <Field ColumnName="regnum"/>
          <Field ColumnName="oricur"/>
        </Fields>
      </DocRelationTableInfo>
      <!--<DocRelationTableInfo	TableName="arm" DataField="arnum" ConditionField="delarnum" DataTableName="delarm"/>-->
      <DocRelationTableInfo TableName="gstm" DataField="gstgrpnum" ConditionField="gstgrpnum">
        <Fields>
          <Field ColumnName="gstgrpname"/>
          <Field ColumnName="gsttype"/>
        </Fields>
      </DocRelationTableInfo>
      <DocRelationTableInfo TableName="acm" DataField="accnum" ConditionField="accnum">
        <Fields>
          <Field ColumnName="accname"/>
        </Fields>
      </DocRelationTableInfo>




      <DocRelationTableInfo TableName="acm" DataField="accnum" ConditionField="accnum2" DataTableName="acm1" />

      <DocRelationTableInfo TableName="sitm" DataField="sitenum" ConditionField="sitenum"  />
      <DocRelationTableInfo TableName="sitm"	DataField="sitenum" ConditionField="sitenum2" DataTableName="sitm2"/>
   
      
			<DocRelationTableInfo TableName="exr" DataField="oricur" ConditionField="oricur">
				<Fields>
					<Field ColumnName="curname"/>
				</Fields>
			</DocRelationTableInfo>
			<DocRelationTableInfo TableName="ptm" DataField="ptnum" ConditionField="payterms"/>
			<DocRelationTableInfo TableName="dorh" DataField="refnum" ConditionField="donum"/>
			<!--<DocRelationTableInfo TableName="mcrh" DataField="refnum" ConditionField="mcrnum"/>-->
		</RelationTables>
		<DocumentPages>
			<!-- sivh -->
			<DocumentPageInfo FileName="Header.xml"		FormName="header" Title="Header">
				<F2Controls>
					<F2Control ControlName="sivh_arnum" F2GridInfo="F2GridInfo_ARM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="arm"/>
						</RelationF2Tables>
					</F2Control>

          <F2Control ControlName="sivh_projectid" F2GridInfo="F2GridInfo_PID.xml">
            <RelationF2Tables>
              <RelationF2Table TableName="viewprojid"/>
            </RelationF2Tables>
          </F2Control>


          <!--<F2Control ControlName="sivh_projectid" F2GridInfo="F2GridInfo_PRJM.xml">
            <RelationF2Tables>
              <RelationF2Table TableName="viewprojid"/>
            </RelationF2Tables>
          </F2Control>-->
          
					<F2Control ControlName="sivh_accnum" F2GridInfo="F2GridInfo_ACM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="acm"/>
						</RelationF2Tables>
					</F2Control>

          <F2Control ControlName="sivh_sitenum" F2GridInfo="F2GridInfo_SITM.xml">
            <RelationF2Tables>
              <RelationF2Table TableName="sitm"/>
            </RelationF2Tables>
          </F2Control>


          <F2Control ControlName="sivh_sitenum2" F2GridInfo="F2GridInfo_SITM.xml">
            <RelationF2Tables>
              <RelationF2Table TableName="sitm2"/>
            </RelationF2Tables>
          </F2Control>


          <F2Control ControlName="sivh_accnum2" F2GridInfo="F2GridInfo_ACM.xml">
            <RelationF2Tables>
              <RelationF2Table TableName="acm2"/>
            </RelationF2Tables>
          </F2Control>
          
					<F2Control ControlName="sivh_payterms" F2GridInfo="F2GridInfo_PTM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="ptm"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="sivh_gstgrpnum" F2GridInfo="F2GridInfo_GSTM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="gstm"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="sivh_oricur" F2GridInfo="F2GridInfo_EXR.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="exr"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="sivh_donum" F2GridInfo="F2GridInfo_DOR.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="dorh"/>
						</RelationF2Tables>
					</F2Control>
					<!--<F2Control ControlName="sivh_mcrnum" F2GridInfo="F2GridInfo_MCR.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="mcrh"/>
						</RelationF2Tables>
					</F2Control>-->
             </F2Controls>
				<!--<Details>
					<Detail DataGridName="dg_SIV1"	DetailGridInfo="DetailGridInfo_SIV1.xml" CaptionText="Sales Invoice Details"/>
				</Details>-->
			</DocumentPageInfo>


			<!--Detail-->
			<DocumentPageInfo FileName="Detail.xml" FormName="detail" Title="Details">
				<Details>
					<Detail DataGridName="dg_SIV1"	DetailGridInfo="DetailGridInfo_SIV1.xml" CaptionText="Sales Invoice Details"/>
				</Details>
			</DocumentPageInfo>

			<!--Cost Centre Distribution-->
			<DocumentPageInfo FileName="CcDist.xml" FormName="CcDist" Title="Cost Centre Distribution">
				<Details>
					<Detail DataGridName="detail_CcDist" DetailGridInfo="DetailGridInfo_CcDist.xml"  CaptionText="Cost Centre Distribution"/>
				</Details>
			</DocumentPageInfo>

			<!-- ard, gld -->
			<DocumentPageInfo FileName="Ledgers.xml"	FormName="ledger" Title="Ledgers">
				<Details>
					<Detail DataGridName="dg_ARD"		DetailGridInfo="DetailGridInfo_ARD.xml"		CaptionText="Debtors (A/R Ledger) Details"/>
					<Detail DataGridName="dg_GLD"		DetailGridInfo="DetailGridInfo_GLD.xml"		CaptionText="General Ledger Details"/>
				</Details>
			</DocumentPageInfo>      
		</DocumentPages>
		<Reports>
			<Report ReportName="Local Invoice - Inclusive GST"		ReportPath="\SIV\Report\invoiceLocal2.rpt" />
			<!--<Report ReportName="Local Invoice - Exclusive GST"		ReportPath="\SIV\Report\invoiceLocalExcludeGst.rpt" />
			<Report ReportName="Oversea Invoice"		ReportPath="\SIV\Report\invoice.rpt" />-->			
		</Reports>
	</DocumentInfo>
</BizXml>
