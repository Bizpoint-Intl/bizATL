<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizF2,BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<F2GridInfo Name="f2GridInfo" MainTable="adh" Title="Search Adhoc Record" PageSize="{languageBinding.sys_F2_Pagesize}" DefaultCondition="adh.[status] = 'P' OR adh.[status] = 'O' and (adh.ctrnum is null or adh.ctrnum='')">
		<F2GridColumnInfo MappingName="refnum"		HeaderText="{languageBinding.refnum}"	Width="100" IsItem="True" SearchItem="True" SortItem="True" Key="True"/>
		<F2GridColumnInfo MappingName="sitenum"	HeaderText="{languageBinding.sitenum}"	Width="200" IsItem="True" />
		<F2GridColumnInfo MappingName="sitename"	HeaderText="{languageBinding.sitename}"	Width="200" IsItem="True" />
		<F2GridColumnInfo MappingName="contractdate"	HeaderText="Contract Date"	Width="200" IsItem="True" />
		<F2GridColumnInfo MappingName="commencedate"	HeaderText="Commencement Date"	Width="200" IsItem="True" />
    <F2GridColumnInfo MappingName="arnum"	HeaderText=""	Width="0" IsItem="True" />
	</F2GridInfo>
</BizXml>
