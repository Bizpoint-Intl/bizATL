		<DetailGridColumnInfo MappingName="date"	HeaderText="Date"	 Width="100"     ColumnType="DateTime" Format="Custom" CustomFormat="dd-MM-yyyy"/>
    <DetailGridColumnInfo MappingName="isOmit"  		  HeaderText="Omit"		  Width="40" 	ColumnType="CheckBox"/>
    <DetailGridColumnInfo MappingName="attnmark"  		  HeaderText="Attn"		  Width="40" 	ColumnType="CheckBox" ReadOnly="True"/>
