    <DetailGridColumnInfo MappingName="adhocremark"	HeaderText="Adhoc Remark"	 Width="150"     Format="#,##0.00####" />




    <DetailGridColumnInfo MappingName="isOT"  		  HeaderText="Is OT"		  Width="40"   	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="OTrate"	HeaderText=""	 Width="0"      Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="isUS"  		  HeaderText=""		  Width="0"  	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="isDRE"  		  HeaderText=""		  Width="40"   	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="isOffset"  		  HeaderText=""		  Width="0"   	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="offsetremark"	HeaderText=""	 Width="0"     Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="isRD"  		  HeaderText=""		  Width="0" 	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="isTR"  		  HeaderText=""		  Width="0"  	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="TRsitenum"	HeaderText="TR Site"  F2="True" F2GridInfo="F2GridInfo_SITM.xml" 	 Width="0"      Format="#,##0.00####" />
		
		
		<DetailGridColumnInfo MappingName="RepRefKey"	HeaderText=""	 Width="0"  ReadOnly="True"       Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="dayofweek"		HeaderText=""			Width="0"	ReadOnly="true"/>
		<DetailGridColumnInfo MappingName="sitenum"		HeaderText="Site"			Width="80"	ReadOnly="true"/>
    <DetailGridColumnInfo MappingName="FixAllowAmt"		HeaderText="Fix Allow"			Width="80"	ReadOnly="true"/>
    
    <DetailGridColumnInfo MappingName="AttnRemarks"	HeaderText="Remark"	 Width="150"     />
		<DetailGridColumnInfo MappingName="empnum3"			  HeaderText="{languageBinding.empnum}"	 ReadOnly="True" 	Width="90" />
		<DetailGridColumnInfo MappingName="refnum"		HeaderText=""			Width="0"	ReadOnly="true" Key="True"/>

		<DetailGridColumnInfo MappingName="paytypecode"	HeaderText="Pay Type"	 Width="40"  F2="True" F2GridInfo="F2GridInfo_PAYTM.xml"    Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="value"	HeaderText="Pay Value"	 Width="50"   ReadOnly="True"    Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="guid"		HeaderText=""			Width="0"	ReadOnly="true" />
		<DetailGridColumnInfo MappingName="atrnum"		HeaderText=""			Width="0"	ReadOnly="true" />
    <DetailGridColumnInfo MappingName="uniquekey"		HeaderText=""			Width="0"	ReadOnly="true" />

    <DetailGridColumnInfo MappingName="Dayoffset"	HeaderText="DayOffset"	 Width="80"     Format="#,##0.00####" />
    
	</DetailGridInfo>
</BizXml>

