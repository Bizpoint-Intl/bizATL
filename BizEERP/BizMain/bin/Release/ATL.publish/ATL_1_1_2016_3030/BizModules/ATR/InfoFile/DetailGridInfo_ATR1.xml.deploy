    <DetailGridColumnInfo MappingName="timein"	HeaderText="Time In"	 Width="70"   ReadOnly="True"    Format="#,##0.00####"/>
    <DetailGridColumnInfo MappingName="schedtimein"	HeaderText="Sched Time In"	 Width="80"   ReadOnly="True"     />
    <DetailGridColumnInfo MappingName="confirmedtimein"	HeaderText="Confirmed Time In"  Width="100"   Format="#,##0.00####"/>
    <DetailGridColumnInfo MappingName="scheddatein"	HeaderText="Sched Date In"	 Width="80"       ColumnType="DateTime" Format="Custom" CustomFormat="dd-MM-yyyy"/>
    <DetailGridColumnInfo MappingName="empnum2"			  HeaderText="{languageBinding.empnum}"	 ReadOnly="True" 	Width="90" />
    <DetailGridColumnInfo MappingName="timeout"	HeaderText="Time Out"	 Width="70"    ReadOnly="True"     Format="#,##0.00####"/>
    <DetailGridColumnInfo MappingName="schedtimeout"	HeaderText="Sched Time Out" ReadOnly="True" 	 Width="80"        />
    <DetailGridColumnInfo MappingName="confirmedtimeout"	HeaderText="Final Time Out"	 Width="100"   Format="#,##0.00####"/>
    <DetailGridColumnInfo MappingName="scheddateout"	HeaderText="Sched Date Out"	 Width="80"       ColumnType="DateTime" Format="Custom" CustomFormat="dd-MM-yyyy"/>
    <DetailGridColumnInfo MappingName="empnum4"			  HeaderText="{languageBinding.empnum}"	 ReadOnly="True" 	Width="90" />
    <DetailGridColumnInfo MappingName="TotalHrs"	HeaderText="Hours"	 Width="60"     ReadOnly="True"    Format="#,##0.00####"/>

    <DetailGridColumnInfo MappingName="LateMins"	HeaderText="Late Hrs"	 Width="60"   ReadOnly="True"      Format="#,##0.00####"/>
    <DetailGridColumnInfo MappingName="OTHrs"	HeaderText="OT Hrs"	 Width="60"   ReadOnly="True"      Format="#,##0.00####"/>

    <DetailGridColumnInfo MappingName="lvmnum"	HeaderText="Status"	 Width="70"       F2="True" F2GridInfo="F2GridInfo_LVM.xml"/>
    <DetailGridColumnInfo MappingName="lvrnum"	HeaderText="Leave No"	 Width="100"       F2="True" F2GridInfo="F2GridInfo_LVR.xml"/>
    <DetailGridColumnInfo MappingName="rnric"	HeaderText=""	 Width="0"  ReadOnly="True"       Format="#,##0.00####" />
    <DetailGridColumnInfo MappingName="rempnum"	HeaderText="Replaced By"	 Width="70"  F2="True" F2GridInfo="F2GridInfo_HEMPH.xml"      Format="#,##0.00####" />
    <DetailGridColumnInfo MappingName="rempname"	HeaderText="Replaced By Name"	 Width="100"    ReadOnly="True"     Format="#,##0.00####" />
