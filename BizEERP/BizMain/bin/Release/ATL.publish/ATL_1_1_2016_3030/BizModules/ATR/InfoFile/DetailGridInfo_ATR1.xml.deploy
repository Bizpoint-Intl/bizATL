<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:rel="BizRAD.BizBase, BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DetailGridInfo Name="detailGridInfo" MainTable="atr1" Title="Time Sheet">
		<RelationTables>
			<rel:RelationTableInfo TableName="lvm" DataField="lvmnum" ConditionField="lvmnum"/>
		</RelationTables>
		<VirtualColumns>
			<rel:VirtualColumn	ColumnName="mark" DataType="System.Boolean" DefaultValue="False"/>
			<rel:VirtualColumn	ColumnName="lvename" DataType="System.String" DefaultValue=""/>
		</VirtualColumns>
		<DetailGridColumnInfo MappingName="mark"  		  HeaderText="{languageBinding.mark}"		  Width="40" 	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="empnum"			  HeaderText="{languageBinding.empnum}"	 F2="True" F2GridInfo="F2GridInfo_HEMPH.xml"  	Width="90" />
		<DetailGridColumnInfo MappingName="empname"	HeaderText="{languageBinding.empname}"	 Width="100"     ReadOnly="True"    Format="#,##0.00####"/>
