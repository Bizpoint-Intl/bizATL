<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDocument, BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DocumentInfo Name="documentInfo" MainTable="trah" Key="refnum"  CompanyTitle="{languageBinding.coyname}" ModuleTitle="{languageBinding.TRA}">
		<RelationTables>
			<DocRelationTableInfo TableName="hemph"		DataField="empnum"	ConditionField="requestedby" DataTableName="hemph"/>
			<DocRelationTableInfo TableName="hemph"		DataField="empnum"	ConditionField="sentby"			DataTableName="hemph1"/>
			<DocRelationTableInfo TableName="hemph"		DataField="empnum"	ConditionField="agreedby"		DataTableName="hemph2"/>
			<DocRelationTableInfo TableName="sitm"		DataField="sitenum"	ConditionField="fromsitenum"/>
			<DocRelationTableInfo TableName="sitm"		DataField="sitenum"	ConditionField="tositenum"	DataTableName="sitm1"/>
      <DocRelationTableInfo TableName="mwt"		DataField="refnum"	ConditionField="refnum"			Save="True"/>
      <DocRelationTableInfo TableName="wac"		DataField="matnum"	ConditionField="matnum"			/>
			<DocRelationTableInfo TableName="ctrh"		DataField="refnum"	ConditionField="ctrnum"/>
			<DocRelationTableInfo TableName="whm"		DataField="whnum"	ConditionField="whnum"/>
      <DocRelationTableInfo TableName="ccm"		DataField="ccnum"	ConditionField="ccnum"/>
      <DocRelationTableInfo TableName="prjm"		DataField="projectid"	ConditionField="projectid"/>
			<DocRelationTableInfo TableName="sem"		DataField="sectorcode"	ConditionField="sectorcode"/>
			<DocRelationTableInfo TableName="grnh"		DataField="refnum"	ConditionField="grnrefnum" Save="True"/>
			<DocRelationTableInfo TableName="vCTRH_ADH" DataField="refnum" ConditionField="docunum" />
			<DocRelationTableInfo TableName="v_TRQH_WPENDING"		DataField="refnum"	ConditionField="trqnum" />
      <DocRelationTableInfo TableName="trqh"		DataField="refnum"	ConditionField="trqnum" />
		</RelationTables>
		<DocumentPages>
			<DocumentPageInfo FileName="header.xml" FormName="header" Title="Header">
				<F2Controls>
					<F2Control ControlName="trah_requestedby"  F2GridInfo="F2GridInfo_HEMPH.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="hemph"/>
						</RelationF2Tables>
					</F2Control>
          <F2Control ControlName="trah_ccnum" F2GridInfo="F2GridInfo_CCM.xml">
            <RelationF2Tables>
              <RelationF2Table TableName="ccm"/>
            </RelationF2Tables>
          </F2Control>
          <F2Control ControlName="trah_projectid" F2GridInfo="F2GridInfo_PRJM.xml">
            <RelationF2Tables>
              <RelationF2Table TableName="prjm"/>
            </RelationF2Tables>
          </F2Control>
					<F2Control ControlName="trah_fromsitenum"  F2GridInfo="F2GridInfo_SITM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="sitm"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="trah_tositenum"  F2GridInfo="F2GridInfo_SITM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="sitm1"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="trah_trqnum" F2GridInfo="F2GridInfo_v_TRQH_WPENDING.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="v_TRQH_WPENDING"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="trah_docunum" F2GridInfo="F2GridInfo_vCTRH_ADH.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="vCTRH_ADH"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="trah_whnum"  F2GridInfo="F2GridInfo_WHM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="whm"/>
						</RelationF2Tables>
					</F2Control>
					<F2Control ControlName="trah_sectorcode"  F2GridInfo="F2GridInfo_SEM.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="sem"/>
						</RelationF2Tables>
					</F2Control>
				</F2Controls>
			</DocumentPageInfo>
      <DocumentPageInfo FileName="detail.xml" FormName="detail" Title="Details">
        <Details>
          <Detail DataGridName="dg_location"  DetailGridInfo="DetailGridInfo_TRA2.xml" CaptionText="Location Extracted"/>
			<Detail DataGridName="dg_item"  DetailGridInfo="DetailGridInfo_TRA1.xml" CaptionText="Items to be Issued"/>
        </Details>
      </DocumentPageInfo>

      <DocumentPageInfo FileName="Ledgers.xml"	FormName="ledger" Title="Ledgers">
        <Details>
          <!--<Detail DataGridName="dg_APD"		DetailGridInfo="DetailGridInfo_APD.xml"		CaptionText="Creditors (A/P Ledger) Details"/>-->
          <Detail DataGridName="dg_GLD"		DetailGridInfo="DetailGridInfo_GLD.xml"		CaptionText="General Ledger Details"/>
        </Details>
      </DocumentPageInfo>
    </DocumentPages>
		<Reports>
      <Report ReportName="Material Issue(Form) - Internal"  DefaultReport="True" ReportPath="\TRA\Report\TraFrm.rpt" />
      <!--	<Report ReportName="Material Issue(Form)"  DefaultReport="True" ReportPath="\TRA\Report\TraFrm2.rpt" />-->
		</Reports>
	</DocumentInfo>
</BizXml>