<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DetailGridInfo Name="detailGridInfo" MainTable="siv1"  ExtractGridInfo="">
		<RelationTables>
			<bas:RelationTableInfo TableName="acm"	DataField="accnum"	ConditionField="accnum"/>
			<bas:RelationTableInfo TableName="matm"	DataField="matnum"	ConditionField="matnum"/>
			<bas:RelationTableInfo TableName="uom"	DataField="uomcode"	ConditionField="uom"/>
		</RelationTables>
		<VirtualColumns>
			<bas:VirtualColumn ColumnName="mark"	DataType="System.Boolean" DefaultValue="False"/>
		</VirtualColumns>

    <DetailGridColumnInfo MappingName="mark"			HeaderText="{languageBinding.mark}"			Width="35"	ColumnType="CheckBox"/>
    <DetailGridColumnInfo MappingName="foc"				HeaderText=""							Width="0"	ColumnType="CheckBox"/>
    <DetailGridColumnInfo MappingName="bold"			HeaderText=""									Width="0"	ColumnType="CheckBox"/>
    <DetailGridColumnInfo MappingName="line"        HeaderText="{languageBinding.line}"           Width="80"      Sort="True"/>
    <DetailGridColumnInfo MappingName="priority"		HeaderText=""								Width="0"/>
    <DetailGridColumnInfo MappingName="ccnum"		HeaderText="Cost Center"		Width="100" F2="True" F2GridInfo="F2GridInfo_CCM.xml"/>
    <DetailGridColumnInfo MappingName="matnum"		HeaderText=""		Width="0" F2="True" F2GridInfo="F2GridInfo_MATM.xml"/>
    <DetailGridColumnInfo MappingName="detail"			HeaderText="Description"						Width="200"/>
    <DetailGridColumnInfo MappingName="oricountry"	HeaderText="Country Of Origin"					Width="75" ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="dqty"			HeaderText="{languageBinding.qty}"			Width="55"	Format ="#,##0.00"/>
    <DetailGridColumnInfo MappingName="uom"			HeaderText="{languageBinding.uom}"			Width="50"	F2="True" F2GridInfo="F2GridInfo_UOM.xml"/>
    <DetailGridColumnInfo MappingName="price"			HeaderText="Price"									Width="80"  Format ="#,##0.00"/>
    <DetailGridColumnInfo MappingName="oricur"			HeaderText="{languageBinding.oricur}"			Width="50"	ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="exrate"		HeaderText="{languageBinding.exrate}"		Width="75"	ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="dgrosamt"		HeaderText="{languageBinding.grosamt}"		Width="75"	Format ="#,##0.00" ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="discamt"		HeaderText="DiscAmt"								Width="75"	Format ="#,##0.00"/>
    <DetailGridColumnInfo MappingName="gstper"		HeaderText="{languageBinding.gstper}"		Width="75" Format ="#,##0.00"/>
    <DetailGridColumnInfo MappingName="roriamt"		HeaderText="{languageBinding.oriamt}"		Width="75"	Format ="#,##0.00" ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="dorigstamt"	HeaderText="Orig-GstAmt"							Width="75"  Format ="#,##0.00"/>
    <DetailGridColumnInfo MappingName="rpostamt"		HeaderText="Post-Amt"								Width="75"	Format ="#,##0.00" ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="dgstamt"		HeaderText="{languageBinding.gstamt}"		Width="75"  Format ="#,##0.00" ReadOnly="True"/>
    <DetailGridColumnInfo MappingName="accnum"		HeaderText="{languageBinding.accnum}"		Width="80"	F2="True" F2GridInfo="F2GridInfo_ACM.xml">
      <RelationF2Columns>
        <RelationF2Column ColumnName="accname"	F2MappingName="accname"/>
      </RelationF2Columns>
    </DetailGridColumnInfo>
    <DetailGridColumnInfo MappingName="accname"		HeaderText="{languageBinding.accname}"	Width="100" ReadOnly="True" TableName="acm"/>

    <DetailGridColumnInfo MappingName="projectid"		HeaderText="Proj ID"		Width="100" F2="True" F2GridInfo="F2GridInfo_PID.xml"/>
    <DetailGridColumnInfo MappingName="sitenum"		HeaderText="{languageBinding.sitenum}"		Width="100" F2="True" F2GridInfo="F2GridInfo_SITM.xml"/>
    <DetailGridColumnInfo MappingName="remark"		HeaderText="Remarks"								Width="200"/>
    <DetailGridColumnInfo MappingName="refnum"		HeaderText="{languageBinding.refnum}"		Width="0"	ReadOnly="True" Key="True"/>

	</DetailGridInfo>
</BizXml>
