<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDocument,	BizBase"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DocumentInfo Name="documentInfo" MainTable="sem" Key="sectorcode" CompanyTitle="{languageBinding.coypg}"  ModuleTitle="{languageBinding.SEM}">
		<RelationTables>
				  
			<DocRelationTableInfo TableName="hemph" DataField="empnum" ConditionField="opmgr">
				<Fields>
					<Field ColumnName="empnum"/>
				</Fields>
			</DocRelationTableInfo>
			
			<DocRelationTableInfo TableName="HEMCR" DataField="country" ConditionField="country">
				<Fields>
					<Field ColumnName="country"/>
				</Fields>
			</DocRelationTableInfo>
		</RelationTables>
		
		<DocumentPages>
			<DocumentPageInfo FileName="Header.xml"		FormName="header"	Title="Header">
				<F2Controls>
					<F2Control ControlName="sem_opmgr" F2GridInfo="F2GridInfo_HEMPH.xml">
						<RelationF2Tables>
							<RelationF2Table TableName="hemph"/>
						</RelationF2Tables>
					</F2Control>
				</F2Controls>

			</DocumentPageInfo>
		</DocumentPages>

	</DocumentInfo>
</BizXml>

