<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizF2,BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<F2GridInfo Name="f2GridInfo" MainTable="hemph" Title="Search Employee Personal Records" PageSize="{languageBinding.sys_F2_Pagesize}" DefaultCondition="status = 'P' OR status = 'O'">
		<F2GridColumnInfo MappingName="empnum"		HeaderText="{languageBinding.empnum}"		Width="100" IsItem="True" SearchItem="True" SortItem="True" />
		<F2GridColumnInfo MappingName="empname"		HeaderText="{languageBinding.empname}"		Width="250" IsItem="True" SearchItem="True" SortItem="True"/>
		<F2GridColumnInfo MappingName="nric"	HeaderText="nric"	Width="100" SearchItem="True" SortItem="True" IsItem="True" Key="True"/>
		<F2GridColumnInfo MappingName="matnum"	HeaderText="{languageBinding.matnum}"	Width="100" IsItem="True"/>
		<F2GridColumnInfo MappingName="sitenum"	HeaderText="{languageBinding.sitenum}"	Width="70" IsItem="True"/>
		<F2GridColumnInfo MappingName="sectorcode"	HeaderText="{languageBinding.sectorcode}"	Width="50" IsItem="True"/>
	</F2GridInfo>
</BizXml>
