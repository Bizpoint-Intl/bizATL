            <ele:BizDisplayValuePair>
              <ele:ComboBoxItems>
                <ele:ComboBoxItem DisplayMember="Jan"	ValueMember="1"/>
                <ele:ComboBoxItem DisplayMember="Feb"	ValueMember="2"/>
                <ele:ComboBoxItem DisplayMember="Mar"	ValueMember="3"/>
