<?xml version="1.0" encoding="utf-8" standalone="no"?>
<BizXml	xmlns:def="Definition" 
		xmlns:ref="Reference"
		xmlns="System.Windows.Forms, System.Windows.Forms"
		xmlns:sd="System.Drawing, System.Drawing" 
		xmlns:s="System, mscorlib" 
		xmlns:ele="BizRAD.BizElements, BizBase" 
		xmlns:dtp="BizRAD.BizControls.BizDateTimePicker, BizControls">
	<Form Name="header" Text="Page" Location="4, 21" Size="736, 535" ForeColor="Black" TabIndex="3" Font="{languageBinding.sys_font}">
		<Controls>
        
			<Label		Name="lbl_expire"	Location="10, 10"	Size="90, 16"	TextAlign="MiddleLeft" Text="{languageBinding.expire}"/>
			<CheckBox	Name="cseh_expire"	Location="110,10"	Size="100,20"	TabIndex="0" FlatStyle="System"/>
      
			<Label		Name="lbl_coursename"	Location="10, 35"	Size="95, 16"	TextAlign="MiddleLeft" Text="Title"/>
			<TextBox	Name="cseh_coursename"	Location="110,35"	Size="250,21"	TabIndex="1"/>
      
      <Label		Name="lbl_coursecentre"	Location="10, 60"	Size="95, 16"	TextAlign="MiddleLeft" Text="Centre"/>
      <TextBox	Name="cseh_coursecentre"	Location="110,60"	Size="250,21"	TabIndex="1"/>
      
      <Label		Name="lbl_coursevenue"	Location="10, 95"	Size="95, 16"	TextAlign="MiddleLeft" Text="Venue"/>
      <TextBox	Name="cseh_coursevenue" Location="110, 95"	Size="320, 60"	TabIndex="6" Multiline="True" ScrollBars="Vertical" 	/>

      <Label		Name="lbl_coursetype"	  Location="10,  160"		Size="50, 16" TextAlign="MiddleLeft" Text="Type"/>
      <ComboBox Name="cseh_coursetype" 	  Location="110,160"		Size="80,21" Enabled="True" TabStop="False">
        <ele:BizDisplayValuePair>
          <ele:ComboBoxItems>
            <ele:ComboBoxItem DisplayMember="NIL"	ValueMember=""/>
            <ele:ComboBoxItem DisplayMember="Full-Time"	ValueMember="Full-Time"/>
            <ele:ComboBoxItem DisplayMember="Part-Time"	ValueMember="Part-Time"/>
          </ele:ComboBoxItems>
        </ele:BizDisplayValuePair>
      </ComboBox>

      <Label		Name="lbl_coursefunding"	Location="10, 185"	Size="95, 16"	TextAlign="MiddleLeft" Text="Funding"/>
      <TextBox	Name="cseh_coursefunding"	Location="110,185"	Size="250,21"	TabIndex="1"/>
      
		</Controls>
	</Form>
</BizXml>