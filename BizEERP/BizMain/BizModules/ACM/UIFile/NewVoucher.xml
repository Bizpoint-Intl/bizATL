<?xml version="1.0" encoding="utf-8" standalone="no"?>
<BizXml	xmlns:def="Definition" 
		xmlns:ref="Reference"
		xmlns="System.Windows.Forms, System.Windows.Forms"
		xmlns:sd="System.Drawing, System.Drawing" 
		xmlns:s="System, mscorlib"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:ele="BizRAD.BizElements, BizBase">
  <Form Name="newvoucher" Text="Select Account Type" AutoScaleBaseSize="5, 14" ClientSize="365, 440" StartPosition="CenterScreen" 
        Font="{languageBinding.sys_font}" FormBorderStyle="FixedToolWindow"	MinimizeBox="false" MaximizeBox="false"	
        ShowInTaskbar="False" Closing="NewVoucher_Closing">
    <Controls>
      <GroupBox		Name="grp_selacctype"	Location="5,	5"	  Size="355, 400">
        <Controls>
          <Label		Name="lbl_incomeexpense"	Location="10,	10"	  Size="250, 16"	  TextAlign="MiddleLeft"	Text="Income and Expense Accounts" Font="{languageBinding.Tahoma825Bold}"/>

          <CheckBox	Name="CKBincome"					Location="10,  35"	Size="250,21"	  TabIndex="0" CheckedChanged="CheckBoxChanged" Checked="True" Text="Income"/>
          <CheckBox	Name="CKBotherincome"			Location="10,  57"	Size="250,21"	  TabIndex="1" CheckedChanged="CheckBoxChanged" Text="Other Income"/>
          <CheckBox	Name="CKBexpense"					Location="10,  79"	Size="250,21"	  TabIndex="2" CheckedChanged="CheckBoxChanged" Text="Expense"/>
          <CheckBox	Name="CKBotherexpense"		Location="10, 101"	Size="250,21"	  TabIndex="3" CheckedChanged="CheckBoxChanged" Text="Other Expense"/>
          <CheckBox	Name="CKBcogs"			      Location="10, 123"	Size="250,21"	  TabIndex="4" CheckedChanged="CheckBoxChanged" Text="Cost of Goods Sold"/>

          <Label		Name="lbl_balancesheet"	  Location="10,	150"	Size="250, 16"	  TextAlign="MiddleLeft"	Text="Balance Sheet Accounts" Font="{languageBinding.Tahoma825Bold}"/>

          <CheckBox	Name="CKBcash"					  Location="10, 172"	Size="250, 21"	  TabIndex="5" CheckedChanged="CheckBoxChanged" Text="Cash"/>
          <CheckBox	Name="CKBbank"			      Location="10, 194"	Size="250, 21"	  TabIndex="6" CheckedChanged="CheckBoxChanged" Text="Bank"/>
          <CheckBox	Name="CKBothercurasset"		Location="10, 216"	Size="250, 21"	  TabIndex="7" CheckedChanged="CheckBoxChanged" Text="Other Current Asset"/>
          <CheckBox	Name="CKBinventoryasset"	Location="10, 238"	Size="250, 21"	  TabIndex="8" CheckedChanged="CheckBoxChanged" Text="Inventory Asset"/>
          <CheckBox	Name="CKBotherasset"		  Location="10, 260"	Size="250, 21"	  TabIndex="9" CheckedChanged="CheckBoxChanged" Text="Non-Current Asset"/>
          <CheckBox	Name="CKBfixedasset"		  Location="10, 282"	Size="250, 21"	  TabIndex="10" CheckedChanged="CheckBoxChanged"  Text="Fixed Asset"/>
          <CheckBox	Name="CKBpayrollliability"  Location="10, 304"	Size="250, 21"	TabIndex="11" CheckedChanged="CheckBoxChanged"  Text="Payroll Liability"/>
          <CheckBox	Name="CKBcurrentliability"  Location="10, 326"	Size="250, 21"	TabIndex="12" CheckedChanged="CheckBoxChanged"  Text="Current Liability"/>
          <CheckBox	Name="CKBlongtermliability" Location="10, 348"	Size="250, 21"	TabIndex="13" CheckedChanged="CheckBoxChanged"  Text="Long-Term Liability"/>
          <CheckBox	Name="CKBequity"          Location="10, 370"	Size="250, 21"	  TabIndex="14" CheckedChanged="CheckBoxChanged"  Text="Equity"/>
        </Controls>
      </GroupBox>
      
      <Button		Name="btn_NewVoucherSave"		Location="250,410"	Size="100,23"	TabIndex="15"	Text="Save"		Click="btn_NewSave_Click"/>
    </Controls>
  </Form>
</BizXml>