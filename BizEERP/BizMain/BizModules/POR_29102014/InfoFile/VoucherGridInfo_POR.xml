		xmlns:def="Definition"
		xmlns:ref="Reference">
	<VoucherGridInfo Name="voucherGridInfo" MainTable="porh" Title="{languageBinding.itemPOR}"	DocumentInfo="DocumentInfo_POR.xml" PageSize="{languageBinding.sys_voucher_PageSize}">
