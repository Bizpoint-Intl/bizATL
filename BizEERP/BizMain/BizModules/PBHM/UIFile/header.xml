<?xml version="1.0" encoding="utf-8" standalone="no"?>
<BizXml xml:lang="zh-CN" 
		xmlns:def="Definition" 
		xmlns="System.Windows.Forms, System.Windows.Forms"
		xmlns:dtp="BizRAD.BizControls.BizDateTimePicker, BizControls"
		xmlns:sd="System.Drawing, System.Drawing" 
		xmlns:s="System, mscorlib"
		xmlns:ele="BizRAD.BizElements, BizBase">
  <Form Name="header" Text="Header" Location="4, 21" Size="736, 535" ForeColor="Black" Font="{languageBinding.sys_font}">
    <Controls>
      <Label    Name="lbl_pbhno"    Location="10,  10" Size="90,  21" TabIndex="1" TextAlign="MiddleLeft" Text="{languageBinding.pbhnum}"  />
      <TextBox  Name="PBHM_PBHNUM"  Location="100, 10" Size="252, 21" TabIndex="2" TabStop="False" ReadOnly="True" CharacterCasing="Upper" />
      <Label    Name="lbl_pbhyear"  Location="10,  32" Size="90,  21" TabIndex="3" TextAlign="MiddleLeft" Text="{languageBinding.pbhyear}" />
      <TextBox  Name="PBHM_PBHYEAR" Location="100, 32" Size="50,  21" TabIndex="4" CharacterCasing="Upper" />

      <DataGrid	Name="dg_Dates"		  Location="85, 0"	Size="732, 450" CaptionText="{languageBinding.datesdetail}" Dock="Bottom" HeaderForeColor="ControlText" TabIndex="100" CaptionBackColor="{languageBinding.captionBackColor}"/>
    </Controls>
  </Form>
</BizXml>