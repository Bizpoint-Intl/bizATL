<?xml version="1.0" encoding="utf-8" standalone="no"?>
<BizXml	xmlns:def="Definition"
		xmlns:ref="Reference"
		xmlns="System.Windows.Forms, System.Windows.Forms"
		xmlns:sd="System.Drawing, System.Drawing"
		xmlns:s="System, mscorlib"
		xmlns:ele="BizRAD.BizElements, BizBase"
		xmlns:dtp="BizRAD.BizControls.BizDateTimePicker, BizControls">
	<Form Name="header" Text="Page" Location="4, 21" Size="736, 535" ForeColor="Black" Font="{languageBinding.sys_font}">
		<Controls>
			<GroupBox Name="grp_CustInfo"	Location="5,10"	Text="Customer Info"	Size="362,250" FlatStyle="System">
				<Controls>
					<Label		Name="lbl_arnum"			Location="20,  30"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Customer Code"/>
					<TextBox	Name="mcrh_arnum"		Location="110, 25"	Size="240,21"	TabIndex="1"/>

					<Label		Name="lbl_arname"		Location="20,  55"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Customer Name"/>
					<TextBox	Name="mcrh_detail"		Location="110, 50"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_address"		Location="20, 80"		Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.address}"/>
					<TextBox	Name="mcrh_addr1"		Location="110, 75"	Size="240,21"	TabStop="False" ReadOnly="True"/>
					<TextBox	Name="mcrh_addr2"		Location="110,100"	Size="240,21"	TabStop="False" ReadOnly="True"/>
					<TextBox	Name="mcrh_addr3"		Location="110,125"	Size="240,21"	TabStop="False" ReadOnly="True"/>
					<TextBox	Name="mcrh_addr4"		Location="110,150"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_phone"			Location="20,180"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.phone}"/>
					<TextBox	Name="mcrh_phone"		Location="110,175"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_fax"				Location=" 20,205"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.fax}"/>
					<TextBox	Name="mcrh_fax"			Location="110,200"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_contact"		Location=" 20,230"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.contact}"/>
					<TextBox	Name="mcrh_contact"	Location="110,225"	Size="240,21"	TabIndex="1"/>

				</Controls>
			</GroupBox>

			<GroupBox	Name="grp_WorInfo"	Location="372, 10"	Text="Transaction Info"	Size="362,250" 	FlatStyle="System">
				
				<Controls>

					<Label		Name="lbl_docunum"			Location="20, 30"		Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.docunum}"/>
					<TextBox	Name="mcrh_docunum"		Location="110,25"	Size="240,21"	TabIndex="1"	TextAlign="Left"/>

					<Label		Name="lbl_trandate"						Location="20,  55"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.trandate}"/>
					<dtp:BizDateTimePicker	Name="mcrh_trandate"	Location="110,50"	Size="120,21"	TabIndex="1" Format="Custom" CustomFormat="{languageBinding.sysDateFormat}"/>

					<Label		Name="lbl_period"				Location="270,50"	Size="40, 16"	TextAlign="MiddleLeft"	Text="Period"/>
					<TextBox	Name="mcrh_period"			Location="320,50"	Size="30,21"	TabIndex="1"	TextAlign="Center"	ReadOnly="True"/>

					<Label		Name="lbl_payterms"			Location="20, 80"		Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.payterms}"/>
					<TextBox	Name="mcrh_payterms"		Location="110,75"	Size="240,21"	TabIndex="1"	TextAlign="Left"/>

					<Label		Name="lbl_gstgrpnum"		Location=" 20,105"	Size=" 90, 16"		TextAlign="MiddleLeft"	Text="{languageBinding.gstgrpnum}"/>
					<TextBox	Name="mcrh_gstgrpnum"	Location="110,100"	Size="70, 21"		TabIndex="1"/>
					<TextBox	Name="mcrh_gstper"			Location="185,100"	Size="170, 21"		TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_oricur"				Location=" 20,130"	Size=" 90, 16"		TextAlign="MiddleLeft"	Text="{languageBinding.oricur}"/>
					<TextBox	Name="mcrh_oricur"			Location="110,125"	Size="70, 21"		TabIndex="1"/>
					<TextBox	Name="mcrh_exrate"			Location="185,125"	Size="170, 21"		TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_posOutlet"		Location=" 20,150"	Size="75, 16"	TextAlign="MiddleLeft"	Text="POS Outlet"/>
					<TextBox	Name="mcrh_outletid"		Location="110,150"	Size="241, 21" TabIndex="1"/>

					<Label		Name="lbl_accnum"			Location=" 20,175"	Size=" 90, 16"		TextAlign="MiddleLeft"	Text="Debit A/C"/>
					<TextBox	Name="mcrh_accnum"		Location="110,175"	Size="70, 21"		TabIndex="1"/>
					<TextBox	Name="acm_accname"		Location="185,175"	Size="170, 21"		TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_posdate"								Location="20,  200"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Date Range"/>
					<dtp:BizDateTimePicker	Name="mcrh_posdatefrom"		Location="110,200"	Size="120,21"	TabIndex="1" Format="Custom" CustomFormat="{languageBinding.sysDateFormat}"/>
					<dtp:BizDateTimePicker	Name="mcrh_posdateto"		Location="235,200"	Size="120,21"	TabIndex="1" Format="Custom" CustomFormat="{languageBinding.sysDateFormat}"/>
					
					<Button		Name="btn_getPOS"			Location="150, 225"	Size="200,23"		TabIndex="1"		Text="Get Monthly Transaction (POS)" 	Click=""/>

				</Controls>
			</GroupBox>
			
			<DataGrid	Name="dg_MCR1"			Location="0, 260"			Size="736,165"	HeaderForeColor="ControlText" TabIndex="1" CaptionBackColor="{languageBinding.captionBackColor}"/>
			
			<GroupBox Name="grp_Remark"	Location="5, 425"	Text="Remarks Info"	Size="362,85" 	FlatStyle="System">
				<Controls>
					<Label		Name="lbl_remark"		Location="20,20"		Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.remarks}"/>
					<TextBox	Name="mcrh_remark"		Location="110,15"	Size="240,55"	TabIndex="1"	Multiline="True"	ScrollBars="Vertical"/>
				</Controls>
			</GroupBox>
			<Label		Name="lbl_grosamt"			Location="515,460"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Gross Amt"/>
			<TextBox	Name="mcrh_grosamt"		Location="615,460"	Size="120,21"	TabStop="False" ReadOnly="True"  TextAlign="Right"/>
			<Label		Name="lbl_origstamt"			Location="515,485"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Orig GST Amt"/>
			<TextBox	Name="mcrh_origstamt"		Location="615,485"	Size="120,21"	TabStop="False" ReadOnly="True"	TextAlign="Right"/>
			<Label		Name="lbl_oriamt"				Location="515,510"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Orig Amt"/>
			<TextBox	Name="mcrh_oriamt"			Location="615,510"	Size="120,21"	TabStop="False" ReadOnly="True"	TextAlign="Right"/>
		</Controls>
	</Form>
</BizXml>