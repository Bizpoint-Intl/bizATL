      <Report ReportName="Stock Adjustment"			ReportPath="\LVA\Report\LVAFrm_Adj.rpt"/>
      <Report ReportName="Stock Take Report"			ReportPath="\LVA\Report\LVAFrm_Take.rpt"/>
    </Reports>-->
	</DocumentInfo>
</BizXml>