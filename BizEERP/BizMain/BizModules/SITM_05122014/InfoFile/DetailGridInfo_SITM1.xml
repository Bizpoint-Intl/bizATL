<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:rel="BizRAD.BizBase, BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DetailGridInfo Name="detailGridInfo" MainTable="sitm1" Title="Inventory">
		<VirtualColumns>
			<rel:VirtualColumn	ColumnName="mark" DataType="System.Boolean" DefaultValue="False"/>
		</VirtualColumns>
		<DetailGridColumnInfo MappingName="mark"  		  HeaderText="{languageBinding.mark}" ReadOnly="True" 		  Width="40" 	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="equipment"	HeaderText="Equipment"	Width="150"  ReadOnly="True"        Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="qty"	HeaderText="Total Stock" ReadOnly="True"  	Width="150"         Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="totalissued"	HeaderText="Total Issued" ReadOnly="True"  	Width="150"         Format="#,##0.00####"/>
		<DetailGridColumnInfo MappingName="balonhand"	HeaderText="Balance on hand" ReadOnly="True"  	Width="150"         Format="#,##0.00####"/>

		<DetailGridColumnInfo MappingName="sitenum"		HeaderText=""			Width="0"	ReadOnly="true" Key="True"/>
		
	</DetailGridInfo>
</BizXml>

