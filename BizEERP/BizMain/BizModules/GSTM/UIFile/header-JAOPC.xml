<?xml version="1.0" encoding="utf-8" standalone="no"?>
<BizXml xml:lang="zh-CN" 
		xmlns:def="Definition" 
		xmlns="System.Windows.Forms, System.Windows.Forms" 
		xmlns:sd="System.Drawing, System.Drawing" 
		xmlns:ele="BizRAD.BizElements, BizBase">
	<Form Name="header" Text="Header" Location="4, 21" Size="736, 535" ForeColor="Black" Font="{languageBinding.sys_font}">
		<Controls>
			<Label    Name="lbl_gstgrpnum"							Location="10,  10"  Size="90, 21"		TextAlign="MiddleLeft" Text="{languageBinding.gstgrpnum}"/>
			<TextBox  Name="GSTM_GSTGRPNUM"			        Location="100, 10"  Size="150,21"		ReadOnly="True" TabStop="False" CharacterCasing="Upper"	/>
			<Label    Name="lbl_gstgrpname"							Location="10,  35"  Size="90, 21"		TextAlign="MiddleLeft" Text="{languageBinding.gstgrpname}"/>
			<TextBox  Name="GSTM_GSTGRPNAME"						Location="100, 35"  Size="250,21"		TabIndex="0" CharacterCasing="Upper"	/>
			<Label    Name="lbl_gsttype"								Location="10,  60"  Size="90, 21"		TextAlign="MiddleLeft" Text="GST Type"/>
			<ComboBox Name="GSTM_gsttype"								Location="100, 60"  Size="150,21"		TabStop="False">
				<ele:BizDisplayValuePair>
					<ele:ComboBoxItems>
						<ele:ComboBoxItem DisplayMember="Taxable"		    ValueMember="1"/>
						<ele:ComboBoxItem DisplayMember="Non-Taxable"		ValueMember="2"/>
						<ele:ComboBoxItem DisplayMember="Manual"				ValueMember="3"/>
					</ele:ComboBoxItems>
				</ele:BizDisplayValuePair>
			</ComboBox>
		</Controls>
	</Form>
</BizXml>