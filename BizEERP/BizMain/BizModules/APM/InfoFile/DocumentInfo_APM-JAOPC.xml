<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDocument,	BizBase"
		xmlns:bas="BizRAD.BizBase,	BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DocumentInfo Name="documentInfo" MainTable="apm" Key="apnum" CompanyTitle="{languageBinding.coyTransview}" ModuleTitle="{languageBinding.itemAPM}">
    <RelationTables>
		<DocRelationTableInfo	TableName="ptm" DataField="ptnum" ConditionField="ptnum">
			<Fields>
				<Field ColumnName="ptname"/>
			</Fields>
		</DocRelationTableInfo>
		<DocRelationTableInfo	TableName="gstm" DataField="gstgrpnum" ConditionField="gstgrpnum">
			<Fields>
				<Field ColumnName="gstgrpname"/>
			</Fields>
		</DocRelationTableInfo>
		<DocRelationTableInfo	TableName="regm" DataField="regnum" ConditionField="regnum">
			<Fields>
				<Field ColumnName="regname"/>
			</Fields>
		</DocRelationTableInfo>
		<DocRelationTableInfo	TableName="exr" DataField="oricur" ConditionField="oricur">
			<Fields>
				<Field ColumnName="curname"/>
			</Fields>
		</DocRelationTableInfo>
		<DocRelationTableInfo	TableName="acm" DataField="accnum" ConditionField="accnum">
			<Fields>
				<Field ColumnName="accname"/>
			</Fields>
		</DocRelationTableInfo>
		<DocRelationTableInfo	TableName="acm" DataField="accnum" ConditionField="accnum2"	DataTableName="acm2">
			<Fields>
				<Field ColumnName="accname"/>
			</Fields>
		</DocRelationTableInfo>
	</RelationTables>
    <DocumentPages>
			<!-- apm -->
			<DocumentPageInfo FileName="Header.xml"	FormName="header" Title="Page">
        <F2Controls>
			<F2Control ControlName="apm_ptnum" F2GridInfo="F2GridInfo_PTM.xml">
				<RelationF2Tables>
					<RelationF2Table TableName="ptm"/>
				</RelationF2Tables>
			</F2Control>
			<F2Control ControlName="apm_gstgrpnum" F2GridInfo="F2GridInfo_GSTM.xml">
				<RelationF2Tables>
					<RelationF2Table TableName="gstm"/>
				</RelationF2Tables>
			</F2Control>
			<F2Control ControlName="apm_regnum" F2GridInfo="F2GridInfo_REGM.xml">
				<RelationF2Tables>
					<RelationF2Table TableName="regm"/>
				</RelationF2Tables>
			</F2Control>
			<F2Control ControlName="apm_oricur" F2GridInfo="F2GridInfo_EXR.xml">
				<RelationF2Tables>
					<RelationF2Table TableName="exr"/>
				</RelationF2Tables>
			</F2Control>
			<F2Control ControlName="apm_accnum" F2GridInfo="F2GridInfo_ACM.xml">
				<RelationF2Tables>
					<RelationF2Table TableName="acm"/>
				</RelationF2Tables>
			</F2Control>
			<F2Control ControlName="apm_accnum2" F2GridInfo="F2GridInfo_ACM.xml">
				<RelationF2Tables>
					<RelationF2Table TableName="acm2"/>
				</RelationF2Tables>
			</F2Control>
        </F2Controls>
			</DocumentPageInfo>
		</DocumentPages>
	</DocumentInfo>
</BizXml>
