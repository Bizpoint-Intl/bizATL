<?xml version="1.0" encoding="utf-8"?>
<BizXml	xmlns="BizRAD.BizDetail, BizBase"
		xmlns:rel="BizRAD.BizBase, BizBase"
		xmlns:def="Definition"
		xmlns:ref="Reference">
	<DetailGridInfo Name="detailGridInfo" MainTable="wor3" Title="FeedBack Lists">
		<VirtualColumns>
			<rel:VirtualColumn	ColumnName="mark" DataType="System.Boolean" DefaultValue="False"/>
		</VirtualColumns>
		<DetailGridColumnInfo MappingName="mark"  		  HeaderText="{languageBinding.mark}"		  Width="40" 	ColumnType="CheckBox"/>
		<DetailGridColumnInfo MappingName="matnum"			  HeaderText="Equipment" 		Width="150" F2="True" F2GridInfo="F2GridInfo_MATMSTK.xml"/>
		<!--<DetailGridColumnInfo MappingName="matnum"			  HeaderText="Equipment" 		Width="150" />-->
		<DetailGridColumnInfo MappingName="matname"	HeaderText="Desc"	Width="100"         Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="estmthcost"	HeaderText="Est Mth ($)"	Width="70"         Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="actmthcost"	HeaderText="Actual Mth ($)"	Width="70"         Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="qty"	HeaderText="Qty"	Width="100"         Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="rate"	HeaderText="Price"	Width="70"         Format="#,##0.00####" />

		<DetailGridColumnInfo MappingName="totalmths"	HeaderText="No of Mths"	Width="70"         Format="#,##0.00####" />

		<DetailGridColumnInfo MappingName="oriamt"	HeaderText=""	Width="0"         Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="moriamt"	HeaderText=""	Width="0"         Format="#,##0.00####" />

		<DetailGridColumnInfo MappingName="amount"	HeaderText="Amount($)" ReadOnly="True" 	Width="100"         Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="remark"	HeaderText="Comments"	Width="200"         Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="refnum"		HeaderText=""			Width="0"	ReadOnly="true" Key="True"/>
	</DetailGridInfo>
</BizXml>

