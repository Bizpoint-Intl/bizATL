		<DetailGridColumnInfo MappingName="licencsestatus"			  HeaderText="License Status"		Width="80" Format="#,##0.00####" />
		<DetailGridColumnInfo MappingName="empnum"	HeaderText=""	Width="0"   Format="#,##0.00####" ReadOnly="true" Key="True"/>
