<?xml version="1.0" encoding="utf-8" standalone="no"?>
<BizXml	xmlns:def="Definition" 
		xmlns:ref="Reference"
		xmlns="System.Windows.Forms, System.Windows.Forms"
		xmlns:sd="System.Drawing, System.Drawing" 
		xmlns:s="System, mscorlib" 
		xmlns:ele="BizRAD.BizElements, BizBase" 
		xmlns:dtp="BizRAD.BizControls.BizDateTimePicker, BizControls">
	<Form Name="header" Text="Page" Location="4, 21" Size="736, 535" ForeColor="Black" Font="{languageBinding.sys_font}">
		<Controls>
			<GroupBox Name="grp_CustInfo"	Location="5,10"	Text="Supplier Info"	Size="362,250" FlatStyle="System">
				<Controls>
					<Label		Name="lbl_apnum"		Location="20,  30"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Supplier Code"/>
					<TextBox	Name="pivh_apnum"		Location="110, 25"	Size="240,21"	TabIndex="1"/>

					<Label		Name="lbl_apname"		Location="20,  55"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Supplier Name"/>
					<TextBox	Name="pivh_detail"		Location="110, 50"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_address"		Location="20, 80"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.address}"/>
					<TextBox	Name="pivh_address"	Location="110, 75"	Size="240,33"	TabStop="False"	Multiline="True"	ScrollBars="Vertical" ReadOnly="True"/>

					<Label		Name="lbl_phone"			Location="20, 117"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.phone}"/>
					<TextBox	Name="pivh_phone"		Location="110,113"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_hp"				Location="20, 142"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.hp}"/>
					<TextBox	Name="pivh_hp"			Location="110,138"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_fax"				Location="20, 167"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.fax}"/>
					<TextBox	Name="pivh_fax"			Location="110,162"	Size="240,21"	TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_contact"		Location="20,192"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.contact}"/>
					<TextBox	Name="pivh_contact"	Location="110,187"	Size="240,21"	TabIndex="1" ReadOnly="false"/>

					<Label		Name="lbl_invdate"			Location="20,  217"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Inv Date"/>
					<dtp:BizDateTimePicker	Name="pivh_invdate"	Location="110,212"	Size="120,21"	TabIndex="1" Format="Custom" CustomFormat="{languageBinding.sysDateFormat}"/>


				</Controls>
			</GroupBox>
      
			<GroupBox	Name="grp_WorInfo"	Location="372, 10"	Text="Transaction Info"	Size="362,250" 	FlatStyle="System">

				<Controls>

					<Label		Name="lbl_docunum"			Location="20, 30"		Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.docunum}"/>
					<TextBox	Name="pivh_docunum"		Location="110,25"	Size="240,21"	TabIndex="1"	TextAlign="Left"/>

					<Label		Name="lbl_supinvnum"			Location="20, 55"		Size="90, 16"	TextAlign="MiddleLeft"	Text="Supplier Inv No."/>
					<TextBox	Name="pivh_supinvnum"			Location="110,50"	Size="240,21"	TabIndex="1"	TextAlign="Left"/>

					<Label		Name="lbl_trandate"			Location="20,  80"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.trandate}"/>
					<dtp:BizDateTimePicker	Name="pivh_trandate"	Location="110,75"	Size="120,21"	TabIndex="1" Format="Custom" CustomFormat="{languageBinding.sysDateFormat}"/>

					<Label		Name="lbl_period"				Location="270,75"	Size="40, 16"	TextAlign="MiddleLeft"	Text="Period"/>
					<TextBox	Name="pivh_period"			Location="320,75"	Size="30,21"	TabIndex="1"	TextAlign="Center"	ReadOnly="True"/>

					<Label		Name="lbl_payterms"			Location="20, 105"	Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.payterms}"/>
					<TextBox	Name="pivh_payterms"		Location="110,100"	Size="240,21"	TabIndex="1"	TextAlign="Left"/>

					<Label		Name="lbl_gstgrpnum"		Location=" 20,130"	Size=" 90, 16"		TextAlign="MiddleLeft"	Text="{languageBinding.gstgrpnum}"/>
					<TextBox	Name="pivh_gstgrpnum"		Location="110,125"	Size="70, 21"		TabIndex="1"/>
					<TextBox	Name="pivh_gstper"			Location="185,125"	Size="170, 21"		TabStop="False" />

					<Label		Name="lbl_oricur"				Location=" 20,155"	Size=" 90, 16"		TextAlign="MiddleLeft"	Text="{languageBinding.oricur}"/>
					<TextBox	Name="pivh_oricur"			Location="110,150"	Size="70, 21"		TabIndex="1"/>
					<TextBox	Name="pivh_exrate"			Location="185,150"	Size="170, 21"		TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_accnum"			Location=" 20,180"	Size=" 90, 16"		TextAlign="MiddleLeft"	Text="Credit A/C"/>
					<TextBox	Name="pivh_accnum"		Location="110,175"	Size="70, 21"		TabIndex="1"/>
					<TextBox	Name="acm_accname"		Location="185,175"	Size="170, 21"		TabStop="False" ReadOnly="True"/>

					<Label		Name="lbl_grnum"			Location=" 20,205"	Size=" 90, 16"		TextAlign="MiddleLeft"	Text="GRN No."/>
					<TextBox	Name="pivh_grnum"		Location="110,200"	Size="240, 21"		TabIndex="1"/>
					
					<CheckBox	Name="pivh_inclgst"			Location="110,  225"	Size="250,21"	  TabIndex="1" Checked="False" Text="GST Inclusive"/>
					
				</Controls>
			</GroupBox>
      
			<DataGrid	Name="dg_PIV1"			Location="0, 260"			Size="736,175"	HeaderForeColor="ControlText" TabIndex="1" CaptionBackColor="{languageBinding.captionBackColor}"/>
      
			<GroupBox Name="grp_Remark"	Location="5, 445"	Text="Remarks Info"	Size="362,85" 	FlatStyle="System">
				<Controls>
					<Label		Name="lbl_remark"		Location="20,20"		Size="90, 16"	TextAlign="MiddleLeft"	Text="{languageBinding.remarks}"/>
					<TextBox	Name="pivh_remark"		Location="110,15"	Size="240,55"	TabIndex="1"	Multiline="True"	ScrollBars="Vertical"/>
				</Controls>
			</GroupBox>

			<Label		Name="lbl_grosamt"			Location="515,435"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Gross Amt"/>
			<TextBox	Name="pivh_grosamt"		Location="615,435"	Size="120,21"	TabStop="False" ReadOnly="True"  TextAlign="Right"/>
			<Label		Name="lbl_discamt"			Location="515,460"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Disc Amt"/>
			<TextBox	Name="pivh_discamt"		Location="615,460"	Size="120,21"	TabStop="False" ReadOnly="True"	TextAlign="Right"/>
			<Label		Name="lbl_origstamt"			Location="515,485"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Orig GST Amt"/>
			<TextBox	Name="pivh_origstamt"		Location="615,485"	Size="120,21"	TabStop="False" ReadOnly="True"	TextAlign="Right"/>
			<Label		Name="lbl_oriamt"				Location="515,510"	Size="90, 16"	TextAlign="MiddleLeft"	Text="Orig Amt"/>
			<TextBox	Name="pivh_oriamt"			Location="615,510"	Size="120,21"	TabStop="False" ReadOnly="True"	TextAlign="Right"/>
		</Controls>
	</Form>
</BizXml>