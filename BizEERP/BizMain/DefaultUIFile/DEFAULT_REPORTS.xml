<?xml version="1.0" encoding="utf-8" ?>
<BizXml xmlns="System.Windows.Forms"
		xmlns:def="Definition"
		xmlns:ref="Reference"
		xmlns:ele="BizRAD.BizElements, BizBase"
		xmlns:lxf="LXFClassLibrary.Controls, MagicLibrary"
		xmlns:mgc="BizRAD.BizMagicLibrary, BizMagicLibrary">
	<Form Name="frm_Reports" Text="{languageBinding.frm_Reports}" Tag="Reports" BackColor="0,78,152">
		<!--ele:BizFormIcon Icon="Bizpoint.ico"/-->
		<Controls>
			<Panel Name="pan_Reports" BackColor="Control" Dock="Fill">
				<Controls>
					<lxf:TabControl Name="tcl_Reports" Appearance="MultiDocument" Dock="Fill" Style="IDE" BackColor="0,78,152" IDEPixelBorder="true" Visible="true" TextColor="Orange" TextInactiveColor="White" Font="{languageBinding.Tahoma875Bold}"/>
					<Splitter Name="slt_Reports" Dock="Left"	MinExtra="100"		MinSize="150" />
					<TreeView Name="tvw_Reports" Dock="Left"	Font="{languageBinding.Tahoma9}"	ImageList="{imageList}" ItemHeight="18" Width="180" LabelEdit="True">
						<Nodes>
							<TreeNode Name="trn_Reports" Text="Reports" ImageIndex="13" SelectedImageIndex="13">
								<Nodes>
									<TreeNode Name="trn_StandardReports"	Text="{languageBinding.trn_StandardReports}"	ImageIndex="12" SelectedImageIndex="12"/>
									<TreeNode Name="trn_CustomReports"		Text="{languageBinding.trn_CustomReports}"		ImageIndex="12" SelectedImageIndex="12"/>
								</Nodes>
							</TreeNode>
						</Nodes>
					</TreeView>
				</Controls>
			</Panel>
			<StatusBar Name="stb_Reports"/>
		</Controls>
	</Form>
</BizXml>
