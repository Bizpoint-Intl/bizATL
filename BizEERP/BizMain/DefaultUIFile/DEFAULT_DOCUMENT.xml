          <Label	 Name="lbl_Document_Status"					      Location="10,160"	Size="150,15" Font="Arial,  8pt" ForeColor="Black" Text="OPEN"/>

          <Label	 Name="lbl_Document_LastUser_Caption"		  Location="10,180"	Size="150,15" Font="Arial,  8pt, style=Bold" ForeColor="Black"	Text="Modified by"/>
          <Label	 Name="lbl_Document_LastUser"				      Location="10,200"	Size="150,15" Font="Arial,  8pt" ForeColor="Black"/>

          <Label	 Name="lbl_Document_LastModified_Caption"	Location="10,220"	Size="150,15" Font="Arial,  8pt, style=Bold" ForeColor="Black"	Text="Modified on"/>
          <Label	 Name="lbl_Document_LastModified"			    Location="10,240"	Size="150,15" Font="Arial,  8pt" ForeColor="Black"/>
        </Controls>
      </GroupBox>

      <Label	 Name="lbl_Document_Total1_Caption"			  Location="350,35"	Size="120,15" Font="Arial,  9pt, style=Bold" ForeColor="0,182,222" Text="Sub Total" Visible="False"/>
      <Label	 Name="lbl_Document_Total2_Caption"			  Location="350,50"	Size="120,15" Font="Arial,  9pt, style=Bold" ForeColor="0,182,222" Text="GST Total" Visible="False"/>
      <Label	 Name="lbl_Document_Total3_Caption"			  Location="350,65"	Size="120,15" Font="Arial,  9pt, style=Bold" ForeColor="0,182,222" Text="Document Total" Visible="False"/>
