      <!--Button-->
      <Button Name="btn_Document_Refresh"		Location="930, 30"	TabIndex="0"	FlatStyle="System"	Text="{languageBinding.btn_Document_Refresh}"	  Font="{languageBinding.Tahoma875Bold}"/>
