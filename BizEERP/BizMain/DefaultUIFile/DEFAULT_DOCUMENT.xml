      <Label	 Name="lbl_Document_Total2"					      Location="470,50"	Size="130,15" Font="Arial,  9pt" ForeColor="White" Text="0.00" TextAlign="MiddleRight" Visible="False"/>
      <Label	 Name="lbl_Document_Total3"					      Location="470,65"	Size="130,15" Font="Arial,  9pt" ForeColor="White" Text="0.00" TextAlign="MiddleRight" Visible="False"/>

      <!--TabControl-->
