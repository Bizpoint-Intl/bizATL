<?xml version="1.0" encoding="utf-8"?>
<BizXml xmlns="System.Windows.Forms"
		xmlns:lxf="LXFClassLibrary.Controls, MagicLibrary"
        xmlns:def="Definition">
  <Form Name="frm_Document" StartPosition="CenterScreen" Tag="Document" AutoScroll="True">
    <Controls>
      <GroupBox	Name="grb_documentDetails" Location="5, 10" Text="" Size="165,270">
        <Controls>
          <!--Title-->
          <Label Name="lbl_Document_Company" Location="10,15" Size="150,40" ForeColor="Black" Font="Arial, 8pt, style=Bold" TextAlign="Center" Multiline="True"/>
          <Label Name="lbl_Document_Module"  Location="10,60" Size="150,40" ForeColor="Black" Font="Arial, 8pt, style=Bold" TextAlign="Center" Multiline="True"/>

          <Label	 Name="lbl_Document_Refnum_Caption"			  Location="10,100"	Size="150,15" Font="Arial,  8pt, style=Bold" ForeColor="Black"	Text="Reference Number"/>
