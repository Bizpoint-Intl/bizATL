      <Button Name="btn_Document_Mark"		  Location="930,410"	TabIndex="13"	FlatStyle="System"	Text="{languageBinding.btn_Document_Mark}"		  Font="{languageBinding.Tahoma875Bold}"/>
      <Button Name="btn_Document_Duplicate"	Location="930,440"	TabIndex="14"	FlatStyle="System"	Text="{languageBinding.btn_Document_Duplicate}"	Font="{languageBinding.Tahoma875Bold}"/>
      <Button Name="btn_Document_Extract"		Location="930,470"	TabIndex="15"	FlatStyle="System"	Text="{languageBinding.btn_Document_Extract}"	  Font="{languageBinding.Tahoma875Bold}"/>


      <Button Name="btn_Document_Print"		  Location="930,520"	TabIndex="16"	FlatStyle="System"	Text="{languageBinding.btn_Document_Print}"		  Font="{languageBinding.Tahoma875Bold}"/>
      <Button Name="btn_Document_Preview"		Location="930,550"	TabIndex="17"	FlatStyle="System"	Text="{languageBinding.btn_Document_Preview}"	  Font="{languageBinding.Tahoma875Bold}"/>

