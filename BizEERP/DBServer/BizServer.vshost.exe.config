<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="DBConnection"      value="Server=localhost;Initial Catalog=SAFYCERP;user id=sa;pwd=sa123;"/>
	</appSettings>
	<system.runtime.remoting>
		<application>
			<channels>
				<channel ref="tcp" port="7880" >
					<serverProviders>
						<provider type="BizRAD.Remoting.CustomServerSinkProvider, BizRAD.Remoting"
							customSinkType="BizRAD.Remoting.CompressionServerSink, BizRAD.Remoting" />
						<formatter type="BizRAD.Remoting.CustomBinaryServerFormatterSinkProvider, BizRAD.Remoting" />
					</serverProviders>
				</channel>
			</channels>
			<service>
				<wellknown type="BizRAD.DB.Service.StandardSQL, BizRAD.DB.Service" objectUri="StandardSQL" mode="Singleton" />
				<wellknown type="BizRAD.DB.Service.DataAccess, BizRAD.DB.Service" objectUri="DataAccess" mode="Singleton" />
				<wellknown type="BizRAD.DB.Service.SpecialSQL, BizRAD.DB.Service" objectUri="SpecialSQL" mode="Singleton" />
				<wellknown type="BizRAD.DB.Service.DataTransaction, BizRAD.DB.Service" objectUri="DataTransaction" mode="Singleton" />
			</service>
		</application>
	</system.runtime.remoting>
</configuration>